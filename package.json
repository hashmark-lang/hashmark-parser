{
	"name": "hashmark",
	"description": "Hashmark",
	"version": "0.0.0",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"license": "MIT",
	"author": "Matthieu Bovel <matthieu@bovel.net>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mbovel/ts-tree-ui.git"
	},
	"dependencies": {
		"@types/benchmark": "^1.0.31",
		"ts-tree": "0.1.8"
	},
	"devDependencies": {
		"@types/mocha": "^5.2.5",
		"@types/node": "^10.9.4",
		"benchmark": "^2.1.4",
		"mocha": "^5.2.0",
		"prettier": "^1.14.2",
		"prettier-check": "^2.0.0",
		"ts-loader": "^4.5.0",
		"ts-node": "^7.0.1",
		"tslint": "^5.11.0",
		"typescript": "^3.0.3"
	},
	"scripts": {
		"prettier-check": "prettier-check **/*.ts **/*.json **/*.yml",
		"prettier": "prettier --write **/*.ts **/*.json **/*.yml",
		"tslint": "tslint --project tsconfig.json",
		"tslint:fix": "tslint --project tsconfig.json --fix",
		"build": "tsc",
		"test": "mocha --recursive",
		"preversion": "npm run prettier-check && npm run tslint",
		"postversion": "git push && git push --tags",
		"benchmark": "node --require ts-node/register benchmarks/parseBenchmark.ts test/input/bigfile.hm"
	}
